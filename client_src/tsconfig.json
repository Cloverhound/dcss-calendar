{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": false,
      "outDir": "./dist/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "lib": [ "es6", "dom" ],
      "target": "es5",
      "strict": true,
      "strictNullChecks": true
    },
    "include": [
      "./src/**/**/*"
    ]
}