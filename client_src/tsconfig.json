{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": false,
      "outDir": "./dist/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es6",
      "strict": true,
      "strictNullChecks": true
    },
    "include": [
      "./src/**/**/*"
    ]
}